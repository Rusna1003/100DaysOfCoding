public class Day8 {
    public static void main(String[] args) {
        //sebelum diupdate
        String nama = "Rusna";
        System.out.println("nama : "+ nama);
       // sesudah diupdate
       nama = "Rusna S";
       System.out.println("nama : "+ nama);
     
       // Sebelum di update 
       int umur = 18;
        System.out.println("umur : "+ umur);
        //Sesudah di update
        umur = 19;
        System.out.println("umur : " + umur);
       
        //Sebelum di update
        short ukuransepatu =36;
        System.out.println("ukuransepatu :" + ukuransepatu);
        // Sesudah di update
        ukuransepatu = 37;
        System.out.println(" ukuransepatu :"+ ukuransepatu);
        
        //Sebelum di update
        double bb = 44.0;
        System.out.println("bb :"+ bb );
        //Sesudah di update
        bb = 45.0;
        System.out.println(" bb :"+ bb);
        
        //Sebelum di update
        long panjang = 20000;
        System.out.println("panjang:"+ panjang);
        //Sesudah di update
        panjang = 300000;
        System.out.println(" panjang :"+ panjang);
        
        
        
       
        
        
        
        
        
    }
    
}
